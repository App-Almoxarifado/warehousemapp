    <!-- Modal -->
    <div class="modal fade" id="plannigModal" tabindex="-1" role="dialog" aria-labelledby="plannigModalTitle"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
            <div class="modal-content" style="border-color: #ffffff;">
                <div class="modal-header">
                    <h5 style="color: #ffffff;">Podemos usar uma obra como referência</h5>
                    <button type="button" class="btn btn-secondary btn-sm" data-dismiss="modal">Fechar</button>
                </div>
                <div class="modal-body">
                    <div class="row justify-content-center">
                        {{#each warehouseSearch}}
                        <div class="col-md-3 mt-1">
                            <div class="card bg-transparent">
                                <div class="card-body">
                                    <div class="row justify-content-center">
                                        <a href="/planning/products/{{_id}}">
                                            <button type="button"
                                                class="btn btn-success btn-sm btn-block">{{description}}
                                            </button>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>

                        {{else}}
                        <div class="container">
                            <h6 style="color: #ffffff;">Nenhuma obra Registrada!!</h6>
                        </div>
                        {{/each}}
                    </div>
                    <hr style="border-color: #ffffff;">
                    <div class="row justify-content-center mt-1">
                        <h6 style="color: #ffffff;">Ou podemos começar um novo pedido do zero, escolha uma das suas
                            obras</h6>
                    </div>
                    <div class="row justify-content-center mt-1">
                        {{#each sites}}
                        <a href="/planning/search/{{_id}}"><button type="button"
                                class="btn btn-primary btn-sm  mr-2">{{description}}</button></a>
                        {{else}}
                        <option value="0">Você não possui nenhuma obra para solicitar Material</option>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>