                    <option value="">Nenhum(a)</option>
                    {{else}}
                    <option value="0">Nenhum tipo de equipamento registrado</option>
                    {{/if_eq}}

                    {{#each types}}
                    <option value="{{_id}}" {{#if_eq ../type _id}}selected{{/if_eq}}>{{description}}</option>
                    {{else}}
                    {{/each}}
