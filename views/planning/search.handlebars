                    {{#each subgroups}}
                    <option value="{{_id}}" {{#if_eq ../subgroup _id}}selected{{/if_eq}}>{{description}}</option>
                    {{else}}
                    <option value="0">Nenhum subgrupo registrado</option>
                    {{/each}}
                </select>
            </div>
            <div class="p-1">
